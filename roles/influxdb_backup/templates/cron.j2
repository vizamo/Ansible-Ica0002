25 1 * * 1,4,7  backup  rm -rf /home/backup/influxdb/*; influxd backup -portable -database telegraf /home/backup/influxdb
25 1 * * 1  backup  duplicity --no-encryption full /home/backup/influxdb/ rsync://{{ host }}@backup.{{ dns_startup_name }}//home/{{ host }}/influxdb/
25 1 * * 4,7  backup  duplicity --no-encryption incremental /home/backup/influxdb/ rsync://{{ host }}@backup.{{ dns_startup_name }}//home/{{ host }}/influxdb/

